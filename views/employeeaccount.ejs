        .nav-link{
          font-size:20px;
          margin-right: 20px;
        }
        .nav-link:hover{
          color:#89CFF0 !important ;
        }
        .dropdown-menu .dropdown-item:hover{
          color: #89CFF0 !important;
        }
         .dropdown-menu .active1{
          color: #89CFF0 !important;
        }
        .active .nav-link{
