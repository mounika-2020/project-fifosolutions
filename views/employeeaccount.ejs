        }
        .emp:hover{
          color:#89CFF0 !important;
        }

        .dropdown:hover .dropdown-menu {
            display: block;
            margin-top: 0; // remove the gap so it doesn't close
         }
         .header{
           position: relative;
           width: 100%;
         }
         .header-inner{
           position: absolute;
           top: 0;
           left: 0;
           width: 100%;
           z-index: 99;
           background-color: transparent;
         }
         .logo{
           width: 40%;
           border-width: 20px;
           color: #ffffff;
         }
         .nav-item .nav-link{
           display: block;
           line-height: 36px;
           text-transform: capitalize;
           font-size: 16px;
           font-weight: 500;
           color: #ffffff;
           transition: 0.15s;
         }
         .nav-item .nav-link:hover{
           color: #FE4066;
         }
         .navbar-scroll{
           background-color: #ffffff;
           padding: 0;
           position: fixed;
           top: 0;
           z-index:99;
           box-shadow: 0px 1px 10px rgba(0,0,0,0.4);
           transition-duration: 0.6s;
         }
         .navbar-scroll .nav-item .nav-link{
