        }
        .right .content{
          margin-left: 45px;
        }
        .right .content .link{
          text-decoration: none;
            color: #d9d9d9;
        }
        .right .content .link:hover{
          text-decoration: none;
          color: #89CFF0;
        }
        @media screen and (max-width: 900px) {
          footer{
            position: relative;
            bottom: 0px;
          }
          .main-content{
            flex-wrap: wrap;
            flex-direction: column;
          }
          .main-content .box{
            margin: 5px 0;
          }
        }

      </style>
